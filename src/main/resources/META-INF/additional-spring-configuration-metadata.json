{
  "hints": [
    {
      "name": "aurora.mvc.header.filter.enabled",
      "values": [
        {
          "value": "true",
          "description": "Enables the Aurora header filter, adds Korrelasjonsid to the log MDC."
        },
        {
          "value": "false",
          "description": "Disables the Aurora header filter."
        }
      ]
    },
    {
      "name": "aurora.mvc.header.resttemplate.interceptor.enabled",
      "values": [
        {
          "value": "true",
          "description": "Enables the RestTemplate interceptor, adds the Korrelasjonsid, Meldingsid & Klientid headers to the request."
        },
        {
          "value": "false",
          "description": "Disables the RestTemplate interceptor."
        }
      ]
    },
    {
      "name": "aurora.mvc.header.span.interceptor.enabled",
      "values": [
        {
          "value": "true",
          "description": "Enables the Zipkin span interceptor, which will add Korrelasjonsid as a tag in the span."
        },
        {
          "value": "false",
          "description": "Disables the Zipkin span interceptor."
        }
      ]
    }
  ]
}